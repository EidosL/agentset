{
  "name": "api",
  "scripts": {
    "build": "pnpm with-env vercel build",
    "clean": "git clean -xdf .cache .next .turbo node_modules",
    "check": "pnpm lint && tsc --noEmit",
    "dev": "pnpm with-env vercel dev",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "start": "pnpm with-env vercel start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@agentset/db": "workspace:*",
    "@t3-oss/env-core": "^0.12.0",
    "hono": "^4.7.5",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@agentset/eslint-config": "workspace:*",
    "@agentset/prettier-config": "workspace:*",
    "@agentset/tsconfig": "workspace:*",
    "dotenv-cli": "^8.0.0",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "vercel": "^32.4.1"
  },
  "prettier": "@agentset/prettier-config"
}
